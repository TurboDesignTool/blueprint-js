{
    "name": "blueprint3d-js",
    "version": "1.0.0",
    "description": "This is the core framework of Blueprint-js for creating floorplans in 2d and 3d",
    "main": "src/scripts/blueprint.js",
    "scripts": {
        "dev": "parcel src/index.html",
        "production": "parcel build src/index.html --no-source-maps",
        "dev-framework": "parcel src/scripts/blueprint.js  --global BP3DJS --no-source-maps",
        "production-framework": "parcel build src/scripts/blueprint.js  --global BP3DJS --no-source-maps",
        "docs": "esdoc"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/aalavandhaann/blueprint-js.git"
    },
    "author": "#0K Srinivasan <ashok.srinivasan2002@gmail.com> (@aalavandhaann)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/aalavandhaann/blueprint-js/issues"
    },
    "devDependencies": {
        "@thi.ng/geom-hull": "0.0.27",
        "babel-core": "^6.26.3",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-latest": "^6.24.1",
        "bezier-js": "^2.4.0",
        "cssnano": "^4.1.10",
        "esdoc": "^1.1.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^7.7.0",
        "eslint-plugin-import": "^2.22.0",
        "event-stream": ">4.0.0",
        "livereload": "^0.7.0",
        "npm-run-all": "^4.0.2",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-static-files-copy": "^2.6.0"
    },
    "dependencies": {
        "@calvinscofield/three-objloader": "^1.0.4",
        "braces": "^2.3.2",
        "es6-enum": "^1.1.0",
        "jquery": "^3.4.1",
        "line-intersect": "^2.2.1",
        "nodeclipse": "^0.17.2",
        "pixi-viewport": "^4.13.2",
        "pixi.js": "^5.3.3",
        "three": "^0.118.3",
        "three-gltf-exporter": "0.0.1",
        "three-reflector": "^1.0.2",
        "three-reflector2": "0.0.6"
    },
    "staticFiles": {
        "staticPath": "build",
        "watcherGlob": "**"
    }
}
